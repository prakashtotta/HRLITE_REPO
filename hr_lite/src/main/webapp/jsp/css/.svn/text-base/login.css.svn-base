.button {
   border-top: 1px solid #96d1f8;
   background: #3B5999;
   background: -webkit-gradient(linear, left top, left bottom, from(#3B5999), to(#3B5999));
   background: -webkit-linear-gradient(top, #3B5999, #3B5999);
   background: -moz-linear-gradient(top, #3B5999, #3B5999);
   background: -ms-linear-gradient(top, #3B5999, #3B5999);
   background: -o-linear-gradient(top, #3B5999, #3B5999);
   padding: 5px 10px;
   -webkit-border-radius: 8px;
   -moz-border-radius: 8px;
   border-radius: 8px;
   -webkit-box-shadow: rgba(0,0,0,1) 0 1px 0;
   -moz-box-shadow: rgba(0,0,0,1) 0 1px 0;
   box-shadow: rgba(0,0,0,1) 0 1px 0;
   text-shadow: rgba(0,0,0,.4) 0 1px 0;
   color: white;
   font-size: 16px;
   font-family:Arial, Helvetica, sans-serif;
   text-decoration: none;
   vertical-align: middle;
   }
.button:hover {
   border-top-color: #3B5998;
   background: #3B5998;
   color: #ccc;
   }
.button:active {
   border-top-color: #1b435e;
   background: #1b435e;
   }

