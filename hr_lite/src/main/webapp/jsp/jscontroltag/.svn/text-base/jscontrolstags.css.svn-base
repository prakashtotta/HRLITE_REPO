/**************************
 *     Autocomplete
***************************/

.autocomplete {
  position: absolute;
  color: #238854;
  border: 1px solid #666;
  overflow: hidden;
  font-family: Verdana; 
  font-size: 12px;
}

.autocomplete ul {
  padding: 0;
  margin: 0;
  list-style: none;
  overflow: auto;
}

.autocomplete li {
  display: block;
  white-space: nowrap;
  cursor: pointer;
  margin: 0px;
  padding-left: 5px;
  padding-right: 5px;
  text-align: left;
  background-color: #FFFFEE;
}

/*
 * CSS used to highlited item when is 
 * selected (with mouse)
*/
.autocomplete li.selected {
  background-color: #B8F888;
  border-top: 1px solid #B8F888;
  border-bottom: 1px solid #B8F888;
}

/*
 * CSS used to highlited keyword
 * found into autocomplete list.
*/
.autocomplete li strong {
	color: red;
}

/*
 * CSS used to display description of keyword.
*/
.autocomplete li code {
	font-family: Verdana; 
	font-size: 11px;
	font-style:italic;
	color:#A0A0A0;
}

/**************************
 *     Slider
***************************/

input.errorNaN {
  border: 1px dotted #666;
  background-color : red;
}

input.errorMax {
  border: 1px dotted #666;
  background-color : green;
}

input.errorMin {
  border: 1px dotted #666;
  background-color : orange;
}

div.sliderTrack {
    background-image: url('../img/slider/slider-inset.png');	
    background-repeat:no-repeat;
    width:151px;
    height:36px;
    position:absolute;
    z-index: 5;
} 

div.sliderTrack2 {
    background-image: url('../img/slider/slider-gradient.png');
    background-repeat:no-repeat;
    width:151px;
    height:22px;
    position:absolute;
    z-index: 10;
} 

div.sliderHandle {
    background: url('../img/slider/volumeslider-handle.png') no-repeat 100%;
    cursor:e-resize;
    width:14px;
    height:20px;
    position:absolute;
    top:20px;
    z-index: 100;
}

div.sliderHandle:hover {
    background: url('../img/slider/volumeslider-handle_glow.png') no-repeat 100%;
    cursor:e-resize;    
    width:14px;
    height:20px;
    position:absolute;
    top:20px;
    z-index: 100;
}

/**************************
 *     Swap
***************************/

/* Style class for list 1 */
ul.list {
	background-color:#f4faef;
	width: 280px;
   	color: black;
    font-size: 12px;
	font-weight: bold;      
	list-style-type: none;
	padding: 4px 4px 0 4px;
	margin: 0px;
	font-family: Arial, sans-serif;
 	border: 1px solid black;
	width:200px;
	height:180px;
	overflow:auto;
}

ul.list li  {
  font-size   : 12px;  
}    

ul.list li.item_selected  {
	color:#FFFFFF;
  	background-color:#238854;
}


ul.list li.item_disabled  {
    filter : alpha(opacity=50);
    -moz-opacity : 0.5;
    opacity : 0.5; 	
}


/* Style class for drop items */
ul.hoverclass {
  border: 1px solid orange;
  background-color:#b5f1d4;  
}

ul.hoverclass li {
	background-color:#b5f1d4;
}

/* Style class for selected items */
div.selectedItems {
  background-color : #cdd7b5;
  height:100%;
}

div.selectedItems ul {
  list-style-type: none;
  padding: 4px 4px 0 4px;
  margin: 0px;
  font-size: 13px;
  font-weight: bold;     
  font-family: Arial, sans-serif;
  border:solid 1px black;
  width:200px;
  background-color: #cdd7b5; 
}

div.selectedItems li {
  height:100%;
  background-color: #cdd7b5;
  color:black;
  border: 0px;
  font-size   : 12px;  
}

/* Style class for swap button */
input.swapButton {
	background-color:#dfddce;		
	font-size:11px;
	color:black;
	width:40px;
	border:solid 1px #8c8c84;	
}

/**************************
 *     Log4j
***************************/
ul.log4j {
  list-style-type: none;
  padding: 4px 4px 0 4px;
  margin: 0px;
  font-size: 13px;
  font-family: Arial, sans-serif;
  border: 1px solid black;
  width:100%;
  height:150px;
  background-color:#E0DDB5;
  overflow:auto;
}

ul.log4j li  {
  font-size   : 11px;  
  background-color: #FFFFEE;
} 

/**************************
 *     TableWrapper (Move Up/Down)
***************************/

/* Style class for list 1 */

table.list1 tr  {
  font-size   : 12px;  
}    

table.list1 tr.item_selected  {
  color:#FFFFFF;
  background-color:#4b4b69;
}

.hoverclass {
  border: 1px solid orange;
  background-color:#a1a1ec;
}
 